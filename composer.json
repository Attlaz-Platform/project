{
  "name": "attlaz/project",
  "description": "Manage and visualize processes",
  "type": "project",
  "version": "1.0.1",
  "require": {
    "php": "^7.2.0",
    "attlaz/project-base": "^1.3.2",
    "attlaz/adapter-base": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "monolog/monolog": "^2.1",
    "bramus/monolog-colored-line-formatter": "~3.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.attlaz.com/"
    }
  ],
  "scripts": {
    "post-autoload-dump": "Attlaz\\Project\\Setup\\Install::run"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
