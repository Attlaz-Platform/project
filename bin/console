#!/usr/bin/env php
<?php
if (PHP_SAPI !== 'cli') {
    echo 'bin/console must be run as a CLI application';
    exit(1);
}

try {
    require __DIR__ . '/../vendor/autoload.php';
} catch (\Exception $e) {
    echo 'Autoload error: ' . $e->getMessage();
    exit(1);
}

use Attlaz\Project;

$project = new Project\Project(dirname(__DIR__));
$project->run();